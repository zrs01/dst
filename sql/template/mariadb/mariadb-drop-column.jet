{{- range .Schemas }}
  {{- range .Tables }}
    {{- table := .Name }}
    {{- range .Columns }}
      {{- parts := split(.ForeignKey, ".") }}
      {{- if len(parts) > 1 }}
ALTER TABLE IF EXISTS {{ table }} DROP CONSTRAINT IF EXISTS fk_{{ table }}_{{ .Name }};
      {{- end }}
ALTER TABLE {{ table }} DROP COLUMN IF EXISTS {{ .Name }}
    {{- end }}
  {{- end }}
{{- end }}
